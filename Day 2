import java.io.*;
import  java.util.*;
class UserMainCode
{
 public int isEven(int input1){
  if(input1%2==0)
   return 2;
  else
   return 1;
                     
 }
}
import java.io.*;
import  java.util.*;
// Read only region start
class UserMainCode
{public int isOdd(int input1){
if(input1%2!=0)
return 2;
else return 1;
}}
